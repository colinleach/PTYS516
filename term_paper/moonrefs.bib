@article{sunday_influence_2022,
	title        = {The influence of gravity on granular impacts - {II}. {A} gravity-scaled collision model for slow interactions},
	author       = {Sunday, Cecily and Murdoch, Naomi and Wilhelm, Arnaud and Drilleau, Melanie and Zhang, Yun and Tardivel, Simon and Michel, Patrick},
	year         = 2022,
	month        = feb,
	journal      = {Astronomy \& Astrophysics},
	volume       = 658,
	pages        = {A118},
	doi          = {10.1051/0004-6361/202142098},
	issn         = {0004-6361, 1432-0746},
	url          = {https://www.aanda.org/articles/aa/abs/2022/02/aa42098-21/aa42098-21.html},
	urldate      = {2022-12-08},
	copyright    = {© C. Sunday et al. 2022},
	note         = {Publisher: EDP Sciences},
	abstract     = {{\textless}i{\textgreater}Context.{\textless}i/{\textgreater} Slow interactions on small body surfaces occur both naturally and through human intervention. The resettling of grains and boulders following a cratering event, as well as observations made during small body missions, can provide clues regarding the material properties and the physical evolution of a surface. In order to analyze such events, it is necessary to understand how gravity influences granular behavior.{\textless}i{\textgreater}Aims.{\textless}i/{\textgreater} In this work, we study slow impacts into granular materials for different collision velocities and gravity levels. Our objectives are to develop a model that describes the penetration depth in terms of the dimensionless Froude number and to use this model to understand the relationship between collision behavior, collision velocity, and gravity.{\textless}i{\textgreater}Methods.{\textless}i/{\textgreater} We used the soft-sphere discrete element method to simulate impacts into glass beads under gravitational accelerations ranging from 9.81 m s{\textless}sup{\textgreater}−2{\textless}sup/{\textgreater} to 0.001 m s{\textless}sup{\textgreater}−2{\textless}sup/{\textgreater}. We quantified collision behavior using the peak acceleration, the penetration depth, and the collision duration of the projectile, and we compared the collision behavior for impacts within a Froude number range of 0–10.{\textless}i{\textgreater}Results.{\textless}i/{\textgreater} The measured penetration depth and collision duration for low-velocity collisions are comparable when the impact parameters are scaled by the Froude number, and the presented model predicts the collision behavior well within the tested Froude number range. If the impact Froude number is low (0 {\textless} Fr {\textless} 1.5), the collision occurs in a regime that is dominated by a depth-dependent quasi-static friction force. If the impact Froude number is high enough (1.5 {\textless} Fr {\textless} 10), the collision enters a second regime that is dominated by inertial drag.{\textless}i{\textgreater}Conclusions.{\textless}i/{\textgreater} The presented collision model can be used to constrain the properties of a granular surface material using the penetration depth measurement from a single impact event. If the projectile size, the collision velocity, the gravity level, and the final penetration depth are known and if the material density is estimated, then the internal friction angle of the material can be deduced.},
	language     = {en},
	file         = {Full Text PDF:C\:\\Users\\colin\\Zotero\\storage\\5SMYXVMM\\Sunday et al. - 2022 - The influence of gravity on granular impacts - II..pdf:application/pdf}
}
