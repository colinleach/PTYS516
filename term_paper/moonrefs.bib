}
@article{pieters_composition_2014,
	series = {Phobos},
	title = {Composition of {Surface} {Materials} on the {Moons} of {Mars}},
	volume = {102},
	issn = {0032-0633},
	url = {https://www.sciencedirect.com/science/article/pii/S0032063314000439},
	doi = {10.1016/j.pss.2014.02.008},
	abstract = {The two small asteroid-like bodies orbiting Mars, Phobos and Deimos, are low albedo and exhibit similar visible to near-infrared spectra. Determining the origin of these moons is closely tied to determining their composition. From available spectroscopic data Phobos exhibits two distinct types of materials across its surface, and data from both Mars Express and Mars Reconnaissance Orbiter have provided additional details about the properties of these materials and their spatial relation to one another. Although no prominent diagnostic absorptions have been detected, systematic weak features are seen in some data. An extensive regolith is observed to have developed on both moons with characteristics that may be unique due to their special environment in Mars orbit. Understanding the character and evolution of the regolith of Phobos and Deimos is central to interpreting the moons׳ physical and optical properties. The cumulative data available for compositional analyses across the surface of Phobos and Deimos, however, remain incomplete in scope and character and ambiguous in interpretation. Consequently the composition of the moons of Mars remains uncertain.},
	language = {en},
	urldate = {2022-12-09},
	journal = {Planetary and Space Science},
	author = {Pieters, Carle M. and Murchie, Scott and Thomas, Nicolas and Britt, Daniel},
	month = nov,
	year = {2014},
	keywords = {Deimos, Phobos, Asteroid capture, Regolith, Space weathering},
	pages = {144--151},
	file = {ScienceDirect Full Text PDF:/home/colin/Zotero/storage/9FNWJS3F/Pieters et al. - 2014 - Composition of Surface Materials on the Moons of M.pdf:application/pdf;ScienceDirect Snapshot:/home/colin/Zotero/storage/M4UFV4GI/S0032063314000439.html:text/html},
}
@article{barucci_phobos_2022,
	title = {Phobos and {Deimos} surface characterization by {MIRS} ({MMX} {InfraRed} {Spectrometer})},
	volume = {44},
	url = {https://ui.adsabs.harvard.edu/abs/2022cosp...44..159B},
	abstract = {Phobos and Deimos will be spectral characterized by MIRS, imaging spectrometer on board of the MMX (Martian Moon eXploration) mission. MMX is an approved sample return mission by JAXA to explore the Martian system (Kuramoto et al. 2022, EPS 74, 12). The spacecraft will be launched in summer 2024, arriving into Mars orbit in 2025 and bringing back Phobos' sample ({\textgreater}10 g) to Earth in 2029. MMX spacecraft will be injected into Phobos co-orbit and then in orbit around Mars like a Martian Moon in the so called quasi-satellite orbits (QSOs). The QSOs will be settled at different altitudes in the equatorial plane of Phobos to observe Phobos at various resolution and select the two landing sites. Observations of the Martian atmosphere will be performed taking the advantage of the quasi-equatorial spacecraft orbits to study spatial and temporal atmosphere changes. Multiple flybys of Deimos will also be performed. One of the major mission goals is to understand the origin of the two moons, which will provide important constrains on planetary formation and on the material transport in the region connecting the inner and outer solar system. The origin of Phobos and Deimos is still a debated question. Past space missions and ground observations revealed that the properties of these two moons shared similarities with asteroids. Their red spectra, without strong absorption features, very different from those of Martian surface, are similar to those of primitive C-D-type asteroids. This similarity together with their characteristics of low albedo and low density, support the hypothesis of the captured asteroids. However their dynamical properties (low inclination and low eccentricity) and more recent numerical simulations seem to be more in favor of in-situ formation by a giant impact on Mars. To characterize the surface composition of the two moons at high spatial resolution and select the two Phobos sampling sites, MIRS will observe the two satellites in the 0.9-3.6 μm range with a spectral resolution better than 20 nm. MIRS will acquire spectra of Phobos at a spatial resolution of about 20 m during the Medium altitude survey (at altitude around 60 km) and of Deimos at spatial resolution of 100 m (at a distance of 300 km). The different landing sites on Phobos will be selected and observed at different resolutions. The high S/N ({\textgreater}100 up to 3.2 μm) and the unprecedented spatial resolution achieved by MIRS will permit to characterize the detailed composition to investigate the local heterogeneity associated with the different surface morphology. MIRS will characterize the surface composition by spectral signatures on both satellites, like hydrous silicate minerals (features at 2.7-2.8 μm, water (ice) (absorption bands at 1.5, 2.0 and 3.0-3.2 μm), minor overtones at 1.4 and 1.8 μm), or anhydrous silicates (bands in the 0.9-1.0 and 2.0 μm regions) as well organic matter (3.3-3.5 μm), if present. MIRS will also be able to measure the spectral radiance of the surface within the instrument footprint. The spectral thermal tail (from about 2.5 μm) will allow us to derive surface temperature variation and surface thermal inertia of Phobos. The MIRS data analysis will contribute to a better understanding of Phobos and Deimos with the characterization of the surface composition material (and subsurface through analysis of crater). The similarity to carbonaceous chondrites with the possible presence of organics and/or ices will imply a capture origin, while the presence, even if partially, of high-temperature phase materials representing a mixture from crust and mantle of Martian silicates would indicate a giant impact origin.},
	urldate = {2022-12-10},
	author = {Barucci, Maria Antonietta and Reess, Jean-Michel and Bernardi, Pernelle and Fornasier, Sonia and Doressoundiram, Alain and Sawyer, Eric and Le Du, Michel and Iwata, Takahiro and Nakagawa, Hiromu and Nakamura, Tomoki},
	month = jul,
	year = {2022},
	note = {Conference Name: 44th COSPAR Scientific Assembly. Held 16-24 July
	ADS Bibcode: 2022cosp...44..159B},
	pages = {159},
}
@article{matsumoto_mmx_2021,
	title = {{MMX} geodesy investigations: science requirements and observation strategy},
	volume = {73},
	issn = {1880-5981},
	shorttitle = {{MMX} geodesy investigations},
	url = {https://doi.org/10.1186/s40623-021-01500-6},
	doi = {10.1186/s40623-021-01500-6},
	abstract = {In order to investigate the origin of Phobos and Deimos, the Japanese Martian Moons eXploration (MMX) mission is scheduled for launch in 2024. MMX will make comprehensive remote-sensing measurements of both moons and return regolith samples from Phobos to Earth. Geodetic measurements of gravity, shape, and rotation parameter of a body provides constraints on its internal structure reflecting its origin and evolution. Moments of inertia are important parameters to constrain the internal mass distribution, but they have not been well determined for the Martian moons yet. We discuss the mission requirements related to the moments of inertia to detect a potential heterogeneity of the mass distribution inside Phobos. We introduce mission instruments and operational strategies to meet the mission requirements. We present a preliminary imaging strategy from a quasi-satellite orbit for a base shape model that is expected to be created at the early stage of the mission. Geodetic products including ephemeris, gravity field, rotation parameter of Phobos, and spacecraft orbit are of importance not only for the geodetic study, but also for interpreting data from various mission instruments and selecting possible landing sites.},
	language = {en},
	number = {1},
	urldate = {2022-12-10},
	journal = {Earth, Planets and Space},
	author = {Matsumoto, Koji and Hirata, Naru and Ikeda, Hitoshi and Kouyama, Toru and Senshu, Hiroki and Yamamoto, Keiko and Noda, Hirotomo and Miyamoto, Hideaki and Araya, Akito and Araki, Hiroshi and Kamata, Shunichi and Baresi, Nicola and Namiki, Noriyuki},
	month = dec,
	year = {2021},
	keywords = {MMX, Gravity, Internal structure, Rotation, Shape},
	pages = {226},
	file = {Full Text PDF:/home/colin/Zotero/storage/RGEA3U7E/Matsumoto et al. - 2021 - MMX geodesy investigations science requirements a.pdf:application/pdf},
}
@article{soter_dust_1971,
